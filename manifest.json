{
"manifest_version": 2,
    "name": "Lisztify",
    "description": "Lisztifies any list.",
    "version": "0.2",
    "icons": {"128": "icon128.png" },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}